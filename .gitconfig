[user]
	name = togashik
	email = togashik@togashi.k

[core]
	editor = /usr/bin/vim

[color]
  ui = true

[alias]
  ad = add
  co = commit
  cam = commit -a --amend
  st = status
  sh = show
  f = flow
  fea = feature
  rel = release
  br = branch
  ch = checkout
  ls = log --stat
  lp = log -p
  d1 = diff HEAD~
  d2 = diff HEAD~~
  d3 = diff HEAD~~~
  d4 = diff HEAD~~~~
  d5 = diff HEAD~~~~~
